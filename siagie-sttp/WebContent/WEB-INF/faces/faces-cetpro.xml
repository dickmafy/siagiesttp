<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
    <!-- JSF and Spring are integrated -->
     <application>
  		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
  	</application>
  	
  	 <!-- Oferta -->
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>ceo_new</from-outcome>
			<to-view-id>/modulos/cetpro/cetpro_new.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>ceo_oferta_update</from-outcome>
			<to-view-id>/modulos/cetpro/cetpro_oferta_update.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>ceo_list</from-outcome>
			<to-view-id>/modulos/cetpro/cetpro_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Itinerario -->
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>cet_iti_new</from-outcome>
			<to-view-id>/modulos/cetpro/itinerario_new.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>cet_iti_upd</from-outcome>
			<to-view-id>/modulos/cetpro/itinerario_update.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>cet_iti_list</from-outcome>
			<to-view-id>/modulos/cetpro/itinerario_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
</faces-config>
