		<navigation-case>
			<from-outcome>adm_pos_upd</from-outcome>
			<to-view-id>/modulos/admision/postulante_update.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>adm_pos_det</from-outcome>
			<to-view-id>/modulos/admision/postulante_detail.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	

	<!-- Matricula -->
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
