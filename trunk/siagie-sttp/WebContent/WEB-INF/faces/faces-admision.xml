			<to-view-id>/modulos/admision/matricula_update.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
			
	<!-- Retiro -->
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>adm_ret_new</from-outcome>
