	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>DocenteSilaboNota</from-outcome>
			<to-view-id>/modulos/intranet/docente/DocenteSilaboNota.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>DocenteSilaboCT</from-outcome>
