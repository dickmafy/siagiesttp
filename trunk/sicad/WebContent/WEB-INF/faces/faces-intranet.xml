<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
    <!-- JSF and Spring are integrated -->
    <application>
  		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
  	</application>
  	
	
	<!-- INTRANET ALUMNO -->
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>AlumnoList</from-outcome>
			<to-view-id>/modulos/intranet/alumno/AlumnoList.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>AlumnoNota</from-outcome>
			<to-view-id>/modulos/intranet/alumno/AlumnoNota.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>AlumnoAsistencia</from-outcome>
			<to-view-id>/modulos/intranet/alumno/AlumnoAsistencia.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
	<!-- INTRANET DOCENTE-->
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
