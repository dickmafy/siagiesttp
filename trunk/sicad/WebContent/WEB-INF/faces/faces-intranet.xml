		<navigation-case>
			<from-outcome>DocenteSilaboAsistenciaListFecha</from-outcome>
			<to-view-id>/modulos/intranet/docente/DocenteSilaboAsistenciaListaFecha.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>DocenteSilaboAsistenciaListaAlumno</from-outcome>
			<to-view-id>/modulos/intranet/docente/DocenteSilaboAsistenciaListaAlumno.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>DocenteAsistenciaAlumnoDetail</from-outcome>
