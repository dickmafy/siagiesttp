<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	   http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- Modulo Intranet -->
	<bean id="docenteSilabo" class="modules.intranet.controller.DocenteSilabo" scope="session" parent="genericController">	  
	    <property name="myService" 	ref="intranetService"/>
	</bean>
	
	<bean id="intranetDocenteAsistencia" class="modules.intranet.controller.IntranetDocenteAsistencia" scope="session" parent="genericController">	  
	    <property name="myService" 	ref="intranetService"/>
	</bean>
	<bean id="intranetDocenteNotas" class="modules.intranet.controller.IntranetDocenteNotas" scope="session" parent="genericController">	  
