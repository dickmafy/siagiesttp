<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	   http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!--  INTRANET -->
	
	<!-- ALUMNO -->
	<bean id="alumnoList" class="modules.intranet.controller.AlumnoList" scope="session" parent="genericController">
	    <property name="bean" ref="metaInstitucional"/>	  
	    <property name="myService" 	ref="horarioService"/>			
	</bean>
	
	<bean id="alumnoNota" class="modules.intranet.controller.AlumnoNota" scope="session" parent="genericController">
	    <property name="myService" 	ref="intranetService"/>
	    <property name="myServiceAdmision" 	ref="admisionService"/>			
	</bean>
	
	<bean id="alumnoAsistencia" class="modules.intranet.controller.AlumnoAsistencia" scope="session" parent="genericController">
	    <property name="myService" 	ref="horarioService"/>			
	</bean>
	
	<!-- DOCENTE -->
	<bean id="docenteSilaboList" class="modules.intranet.controller.DocenteSilaboList" scope="session" parent="genericController">
		<property name="bean" ref="metaInstitucional"/>	  
	    <property name="myService" 	ref="horarioService"/>		
	</bean>
	
	<bean id="docenteSilaboFecha" class="modules.intranet.controller.DocenteSilaboFecha" scope="session" parent="genericController">
		<property name="bean" ref="seccion"/>
	    <property name="myService" 	ref="horarioService"/>	
	</bean>

	<bean id="docenteSilaboNota" class="modules.intranet.controller.DocenteSilaboNota" scope="session" parent="genericController">	  
	    <property name="myService" 	ref="intranetService"/>
	    <property name="myServiceAdmision" 	ref="admisionService"/>
	</bean>
	
	<bean id="docenteSilaboCT" class="modules.intranet.controller.DocenteSilaboCT" scope="session" parent="genericController">	  
	    <property name="myService" 	ref="intranetService"/>
	    <property name="myServiceAdmision" 	ref="admisionService"/>
	</bean>
	
	<bean id="docenteSilaboAsistenciaListFecha" class="modules.intranet.controller.DocenteSilaboAsistenciaListFecha" scope="session" parent="genericController">	  
	    <property name="bean" ref="asistenciaAlumnoCalendario"/>
	    <property name="myService" 	ref="intranetService"/>
	    <property name="myServiceAdmision" 	ref="admisionService"/>
	</bean>
	
	<bean id="docenteSilaboAsistenciaListaAlumno" class="modules.intranet.controller.DocenteSilaboAsistenciaListaAlumno" scope="session" parent="genericController">	  
	    <property name="bean" ref="asistenciaAlumnoCalendario"/>
	    <property name="myService" 	ref="intranetService"/>
	</bean>
	



	<!-- DOCENTE  OTROS-->
	<bean id="docenteSilaboCronograma" class="modules.intranet.controller.DocenteSilaboCronograma" scope="session" parent="genericController">
		<property name="bean" ref="seccion"/>
	    <property name="myService" 	ref="horarioService"/>	
	</bean>
	
	<bean id="docenteSilabo" class="modules.intranet.controller.DocenteSilabo" scope="session" parent="genericController">	  
	    <property name="myService" 	ref="intranetService"/>
	</bean>
	
	
	
<!-- CETPRO -->
	<bean id="intranetCetpro" class="modules.intranet.controller.IntranetCetpro" scope="session" parent="genericController">

	</bean>
	

	
</beans>