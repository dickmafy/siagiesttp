	</bean>
	<bean id="intranetAlumnoAsistencia" class="modules.intranet.controller.IntranetAlumnoAsistencia" scope="session" parent="genericController">	  
	    <property name="myService" 	ref="intranetService"/>
	</bean>
	<bean id="intranetAlumnoNotas" class="modules.intranet.controller.IntranetAlumnoNotas" scope="session" parent="genericController">	  
	    <property name="myService" 	ref="intranetService"/>
	</bean>
	
	<bean id="docenteSilaboCronograma" class="modules.intranet.controller.DocenteSilaboCronograma" scope="session" parent="genericController">
		<property name="bean" ref="seccion"/>
