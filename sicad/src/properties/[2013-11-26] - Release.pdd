<ENTITY ID="11930" Name="m_transversal" SchemaOID="2214" SchemaName="marco" OwnerID="0" OwnerName="" TablespaceID="0" XPos="720" YPos="1600" Temporary="0" OnCommit="" WithOIDsSpecified="0" WithOIDs="0" FillFactor="0" Inherits="" DelayKeyTblUpdates="0" FillColor="16777200" LineColor="8388608" CharacterSet="" Collate="" Comments="Tabla donde se almacena el esquema transveral:modulos, capacidad terminal, criterio de evaluacion, contenidos basicos." Description="" Annotation="" BeginScript="" EndScript="CREATE FUNCTION marco.serial_transversal() RETURNS trigger AS $emp_stamp$\nBEGIN\n    IF NEW.tipo=1 THEN\n\009SELECT INTO NEW.modulo COALESCE(max(modulo)+1, 1) myalias FROM marco.m_transversal WHERE tipo=NEW.tipo;\009\009\009\009\009\n    ELSE\n       IF NEW.nivel=0 AND NEW.subnivel=0 THEN\n\009SELECT INTO NEW.nivel COALESCE(max(nivel)+1, 1) myalias FROM marco.m_transversal WHERE tipo=NEW.tipo;\n       ELSIF NEW.nivel\g0 AND NEW.subnivel=0 THEN\n\009SELECT INTO NEW.subnivel COALESCE(max(subnivel)+1, 1) myalias FROM marco.m_transversal WHERE tipo=NEW.tipo AND nivel=NEW.nivel;\n       END IF;\n     END IF;\n     NEW.pk_transversal:=NEW.subnivel+NEW.nivel*100+NEW.tipo*10000+NEW.modulo*100000;\n     RETURN NEW;\nEND;\n$emp_stamp$ LANGUAGE plpgsql;\n\nCREATE TRIGGER before_transversal\n  BEFORE INSERT\n  ON marco.m_transversal\n  FOR EACH ROW\n  EXECUTE PROCEDURE marco.serial_transversal();\n\nINSERT INTO marco.m_transversal(tipo,modulo,nivel,subnivel,titulo,descripcion,estado) VALUES\n(\a1\a,\a0\a,\a0\a,\a0\a,\aFORMACI\211N Y ORIENTACI\211N\a,\a\a,\a1\a),\n(\a2\a,\a1\a,\a0\a,\a0\a,\a\a,\aRECONOCER, INTERPRETAR Y APLICAR EL MARCO LEGAL QUE DESCRIBE LOS DERECHOS Y OBLIGACIONES EN LAS RELACIONES LABORALES.\a,\a1\a),\n(\a2\a,\a1\a,\a0\a,\a0\a,\a\a,\aIDENTIFICAR Y UTILIZAR LOS PROCEDIMIENTOS DE INSERCI\211N Y DE ORIENTACI\211N EN EL MERCADO LABORAL Y LAS POSIBILIDADES DE EMPLEO\a,\a1\a),\n(\a2\a,\a1\a,\a0\a,\a0\a,\a\a,\aRECONOCER Y APLICAR MEDIDAS DE PREVENCI\211N  DE RIESGOS Y SALUD  OCUPACIONAL INDISPENSABLES PARA EL DESEMPE\209O LABORAL\a,\a1\a),\n(\a2\a,\a1\a,\a1\a,\a0\a,\a\a,\aINTERPRETA, UTILIZA Y VALORA LAS FUENTES B\193SICAS DE INFORMACI\211N DEL DERECHO LABORAL EXAMINANDO LOS DEBERES Y DERECHOS QUE INVOLUCREN A LAS PARTES\a,\a1\a),\n(\a2\a,\a1\a,\a1\a,\a0\a,\a\a,\aANALIZA E INTERPRETA LAS CAUSAS DE SUSPENSI\211N Y T\201RMINO DE UNA RELACI\211N LABORAL.\a,\a1\a),\n(\a2\a,\a1\a,\a1\a,\a0\a,\a\a,\aIDENTIFICA, INTERPRETA Y ELABORA UNA LIQUIDACI\211N DE BENEFICIOS SOCIALES.\a,\a1\a),\n(\a2\a,\a1\a,\a1\a,\a0\a,\a\a,\aDIFERENCIA, EXPLICA Y VALORA LAS PRESTACIONES Y OBLIGACIONES RELATIVAS A ESSALUD, AFP,  SUNAT, ONP, ETC.\a,\a1\a),\n(\a2\a,\a1\a,\a2\a,\a0\a,\a\a,\aIDENTIFICA Y ANALIZA  LA OFERTA Y LA DEMANDA LABORAL Y LO RELACIONA CON SU PERFIL PROFESIONAL.\a,\a1\a),\n(\a2\a,\a1\a,\a2\a,\a0\a,\a\a,\aDESCRIBE Y APLICA LOS PROCEDIMIENTOS DE SELECCI\211N DE LAS FUENTES DE TRABAJO EN EL MERCADO LABORAL\a,\a1\a),\n(\a2\a,\a1\a,\a2\a,\a0\a,\a\a,\aVALORA Y ESTABLECE RASGOS COMO PROFESIONAL Y ASUME CON RESPONSABILIDAD Y RESPETO LA B\218SQUEDA DE EMPLEO E INSERCI\211N EN EL MERCADO LABORAL\a,\a1\a),\n(\a2\a,\a1\a,\a2\a,\a0\a,\a\a,\aDESCRIBE Y RECONOCE LAS CARACTER\205STICAS DE LA PERSONA CON CAPACIDADES EMPRENDEDORAS.\a,\a1\a),\n(\a2\a,\a1\a,\a3\a,\a0\a,\a\a,\aIDENTIFICA  Y EXPLICA LAS SITUACIONES DE RIESGO M\193S COMUNES EN SU ENTORNO DE TRABAJO.\a,\a1\a),\n(\a2\a,\a1\a,\a3\a,\a0\a,\a\a,\aCONTRASTA Y EXPLICA LOS DA\209OS OCASIONADOS POR LAS ACTIVIDADES  LABORALES.\a,\a1\a),\n(\a2\a,\a1\a,\a3\a,\a0\a,\a\a,\aORGANIZA Y APLICA MEDIDAS DE PREVENCI\211N DE RIESGOS DE LA SALUD BIOPSICOSOCIAL DENTRO DEL AMBIENTE LABORAL\a,\a1\a),\n(\a2\a,\a1\a,\a3\a,\a0\a,\a\a,\aIDENTIFICA Y ASUME CON RESPONSABILIDAD LAS MEDIDAS DE PREVENCI\211N COMO UN ESTILO DE VIDA.\a,\a1\a),\n(\a3\a,\a1\a,\a0\a,\a0\a,\a\a,\aLEGISLACI\211N Y RELACIONES LABORALES\nNORMAS LABORALES. DERECHOS Y DEBERES DE L TRABAJADOR\nTEXTO \218NICO ORDENADO DE LA LEY DE FOMENTO DE EMPLEO.\nLA RELACI\211N LABORAL Y CIVIL: MODALIDADES Y TIPOS DE CONTRATOS. SUSPENSI\211N Y EXTINCI\211N.\nSEGURIDAD  SOCIAL Y OTRAS PRESTACIONES\nCONVENIOS LABORALES. NEGOCIACI\211N COLECTIVA\nPRESTACI\211N Y BONIFICACIONES\nORIENTACI\211N E INSERCI\211N SOCIOLABORAL\nEL MERCADO LABORAL. PERSPECTIVAS DEL ENTORNO.\nMARKETING PERSONAL. EL PERFIL PROFESSIONAL\nB\218SQUEDA DE EMPLEO: FUENTES DE INFORMACI\211N Y MECANISMOS DE SELECCI\211N.\nCONVENIOS Y SERVICIOS LABORALES\nAN\193LISIS Y EVALUACI\211N DEL PROPIO POTENCIAL PROFESIONAL Y DE LOS INTERESES PERSONALES. LA SUPERACI\211N DE H\193BITOS SOCIALES DISCRIMINATORIOS. ELABORACI\211N DEL ITINERARIO DE FORMACI\211N PROFESIONAL.\nEL EMPRENDIMIENTO.\nRIESGOS LABORALES, SEGURIDAD Y SALUD OCUPACIONAL\nMEDIDAS DE SEGURIDAD\nRIESGOS EN EL CENTRO DE TRABAJO\nERGONOM\205A\nACCIDENTES LABORALES\nENFERMEDAD LABORAL\nMEDIDAS DE PREVENCI\211N\nSALUD F\205SICA, MENTAL Y SOCIAL EN EL CENTRO DE TRABAJO\nPRIMEROS AUXILIOS\nNORMAS DE BIOSEGURIDAD\nDEFENSA CIVIL\nAUTOCUIDADO\nCALIDAD DE VIDA\n\a,\a1\a),\n(\a1\a,\a0\a,\a0\a,\a0\a,\aGESTI\211N EMPRESARIAL\a,\aCAPACIDADES PARA PRACTICAR UNA ACTITUD EMPRENDEDORA Y COMPETITIVA, ORGANIZAR, ADMINISTRAR  Y GESTIONAR EMPRESAS.\a,\a1\a),\n(\a2\a,\a2\a,\a0\a,\a0\a,\a\a,\aIDENTIFICAR UNA IDEA DE NEGOCIO EN EL MARCO DE LAS DIFERENTES FORMAS JUR\205DICAS DE CONSTITUCI\211N DE EMPRESA,  RECONOCIENDO  SUS PROPIAS CAPACIDADES E INTER\201S ANALIZANDO EL ENTORNO EMPRESARIAL\a,\a1\a),\n(\a2\a,\a2\a,\a0\a,\a0\a,\a\a,\aDETERMINAR EL SEGMENTO DE MERCADO Y APLICAR T\201CNICAS DE RELACI\211N CON LOS CLIENTES Y PROVEEDORES, QUE PERMITAN RESOLVER SITUACIONES COMERCIALES  Y ANALIZAR LAS FORMAS M\193S USUALES DE PROMOCI\211N DE VENTA DE PRODUCTOS O SERVICIOS EN EL SECTOR, ELABORANDO LAS HERRAMIENTAS NECESARIAS.\a,\a1\a),\n(\a2\a,\a2\a,\a0\a,\a0\a,\a\a,\aDETERMINAR LOS PRECIOS DEL PRODUCTO O SERVICIO, A PARTIR DEL C\193LCULO DE LOS COSTOS, UTILIZANDO LOS INSTRUMENTOS NECESARIOS PARA DETERMINAR LA INVERSI\211N Y EL FINANCIAMIENTO EMPRESARI