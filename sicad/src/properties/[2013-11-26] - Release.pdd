<ENTITY ID="47357" Name="m_referente_educativo" SchemaOID="2214" SchemaName="marco" OwnerID="0" OwnerName="" TablespaceID="0" XPos="720" YPos="1400" Temporary="0" OnCommit="" WithOIDsSpecified="0" WithOIDs="0" FillFactor="0" Inherits="" DelayKeyTblUpdates="0" FillColor="16777200" LineColor="8388608" CharacterSet="" Collate="" Comments="Tabla donde se almacena el referente educativo del perfil profesional: modulos, capacidad terminal, criterio de evaluacion y contenidos basicos." Description="" Annotation="" BeginScript="" EndScript="CREATE FUNCTION marco.serial_educativo() RETURNS trigger AS $emp_stamp$\nBEGIN\n     IF NEW.nivel_a=0 AND NEW.nivel_b=0 THEN\n\009SELECT INTO NEW.nivel_a COALESCE(max(nivel_a)+1, 1) myalias FROM marco.m_referente_educativo WHERE pk_profesion=NEW.pk_profesion AND tipo=NEW.tipo;\009\009\009\009\009\n     ELSIF NEW.nivel_a\g0 AND NEW.nivel_b=0 THEN\n\009SELECT INTO NEW.nivel_b COALESCE(max(nivel_b)+1, 1) myalias FROM marco.m_referente_educativo WHERE pk_profesion=NEW.pk_profesion AND tipo=NEW.tipo AND nivel_a=NEW.nivel_a;\009\009\009\009\009\n     ELSIF NEW.nivel_a\g0 AND NEW.nivel_b\g0 THEN\n\009SELECT INTO NEW.nivel_c COALESCE(max(nivel_c)+1, 1) myalias FROM marco.m_referente_educativo WHERE pk_profesion=NEW.pk_profesion AND tipo=NEW.tipo AND nivel_a=NEW.nivel_a AND nivel_b=NEW.nivel_b;\009\009\009\009\009\009\009\009\n     END IF;\n     NEW.pk_referente:=NEW.nivel_c::int8 + NEW.nivel_b*100::int8 + NEW.nivel_a*10000::int8 + NEW.tipo*1000000::int8 + NEW.pk_profesion*10000000::int8;\n     RETURN NEW;\nEND;\n$emp_stamp$ LANGUAGE plpgsql;\n\nCREATE TRIGGER before_educativo\n  BEFORE INSERT\n  ON marco.m_referente_educativo\n  FOR EACH ROW\n  EXECUTE PROCEDURE marco.serial_educativo();\n\nINSERT INTO marco.m_referente_educativo(pk_profesion,tipo,nivel_a,nivel_b,nivel_c,titulo,descripcion,estado) VALUES\n(\a602\a,\a1\a,\a0\a,\a0\a,\a0\a,\aGESTION DEL SOPORTE Y SEGURIDAD DE LAS TECNOLOGIAS DE LA INFORMACI\211N Y COMUNICACI\211N\a,\aADMINISTRAR, GESTIONAR E IMPLEMENTAR, EL SERVICIO DE MANTENIMIENTO Y OPERATIVIDAD DE LOS RECURSOS DE HARDWARE Y SOFTWARE, REDES DE COMUNICACI\211N Y LOS LINEAMIENTOS Y POL\205TICAS DE SEGURIDAD DE LA INFORMACI\211N, TENIENDO EN CUENTA LOS CRITERIOS Y EST\193NDARES VIGENTES.\a,\a1\a),\n(\a602\a,\a1\a,\a0\a,\a0\a,\a0\a,\aDESARROLLO DE SOFTWARE Y GESTI\211N DE BASE DE DATOS\a,\aANALIZAR, DISE\209AR, DESARROLLAR SISTEMAS DE INFORMACI\211N  Y ADMINISTRAR SISTEMAS DE GESTI\211N DE BASE DE DATOS, DE ACUERDO A LOS REQUERIMIENTOS DE LA ORGANIZACI\211N; CONSIDERANDO LOS CRITERIOS DE SEGURIDAD EN LA TRANSMISI\211N Y EL ALMACENAMIENTO DE DATOS.\a,\a1\a),\n(\a602\a,\a1\a,\a0\a,\a0\a,\a0\a,\aGESTION DE APLICACIONES PARA INTERNET Y PRODUCCION MULTIMEDIA\a,\aDISE\209AR, DESARROLLAR, ADMINISTRAR , GESTIONAR E IMPLEMENTAR  PRODUCTOS MULTIMEDIA Y APLICACIONES PARA INTERNET, TENIENDO EN CUENTA LOS REQUERIMIENTOS DEL CLIENTE.\a,\a1\a),\n(\a602\a,\a1\a,\a1\a,\a0\a,\a0\a,\a\a,\aPLANIFICAR EL SERVICIO DE SOPORTE T\201CNICO PARA LA OPTIMIZACI\211N DE LOS RECURSOS\a,\a1\a),\n(\a602\a,\a1\a,\a1\a,\a0\a,\a0\a,\a\a,\aCLASIFICAR LOS RECURSOS INFORM\193TICOS, TENIENDO EN CUENTA LAS NECESIDADES DE TRABAJO, PARA LA INTEGRACI\211N DE LAS TIC A LOS PROCESOS DE LA EMPRESA\a,\a1\a),\n(\a602\a,\a1\a,\a1\a,\a0\a,\a0\a,\a\a,\aELABORAR Y EJECUTAR EL PLAN DE MANTENIMIENTO, CONSIDERANDO LAS CARACTER\205STICAS T\201CNICAS.\a,\a1\a),\n(\a602\a,\a1\a,\a1\a,\a0\a,\a0\a,\a\a,\aANALIZAR E IDENTIFICAR EL PROBLEMA T\201CNICO RESOLVIENDO O VIABILIZANDO LA SOLUCI\211N, DE ACUERDO AL PLAN DE CONTINGENCIA.\a,\a1\a),\n(\a602\a,\a1\a,\a1\a,\a0\a,\a0\a,\a\a,\aELABORAR PROGRAMAS DE INSTRUCCI\211N EN EL USO DE LOS NUEVOS RECURSOS INFORM\193TICOS.\a,\a1\a),\n(\a602\a,\a1\a,\a1\a,\a0\a,\a0\a,\a\a,\aDISE\209AR REDES DE COMUNICACI\211N, DE ACUERDO A LOS EST\193NDARES ESTABLECIDOS. \a,\a1\a),\n(\a602\a,\a1\a,\a1\a,\a0\a,\a0\a,\a\a,\aINSTALAR, CONFIGURAR Y MODIFICAR LA CONECTIVIDAD DE LAS REDES DE COMUNICACI\211N.\a,\a1\a),\n(\a602\a,\a1\a,\a1